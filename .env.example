# Copy this file to .env and update the values

# Database Configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=your_secure_database_password_here

# PostgreSQL Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_database_password_here

# n8n Configuration
N8N_HOST=your-domain.com
N8N_PORT=5678
N8N_PROTOCOL=https
NODE_ENV=production

# Security - Generate a secure random key (at least 32 characters)
N8N_ENCRYPTION_KEY=your_very_secure_encryption_key_change_this_immediately

# Timezone
TZ=UTC

# Webhook URL - Replace with your actual domain
WEBHOOK_URL=https://your-domain.com/

# Metrics (optional)
N8N_METRICS=true

# User Management (optional)
N8N_USER_MANAGEMENT_DISABLED=false

# Log Level
N8N_LOG_LEVEL=info
